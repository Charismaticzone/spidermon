{% import 'slack/spider/notifier/macros.jinja' as renderer with context %}
{% if monitors_failed %}
    :skull: `{{ data.spider.name }}` *spider finished with errors!*{{ renderer.render_job_url() }} _(errors={{ result.monitors_failed_results|length }})_
{% else %}
    :thumbsup: `{{ data.spider.name }}` *spider finished!*{{ renderer.render_job_url() }}
{% endif %}